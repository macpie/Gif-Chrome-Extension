{
  "name": "Gif-Chrome-Extension",
  "version": "4.0.1",
  "private": true,
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-react": "^6.11.1",
    "enzyme": "^2.4.1",
    "grunt": "^1.0.1",
    "grunt-contrib-compress": "^1.3.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-exec": "^1.0.0",
    "grunt-json-replace": "^0.1.2",
    "jest-cli": "^14.1.0",
    "react-addons-test-utils": "^15.3.1",
    "react-scripts": "0.2.1"
  },
  "dependencies": {
    "flux": "^2.1.1",
    "immutable": "^3.8.1",
    "lodash": "^4.14.2",
    "node-uuid": "^1.4.7",
    "promise": "^7.1.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-infinite": "^0.9.2",
    "react-router": "^2.6.1",
    "superagent": "^2.1.0"
  },
  "scripts": {
    "start": "./node_modules/.bin/react-scripts start",
    "build": "./node_modules/.bin/grunt build-extension",
    "watch": "./node_modules/.bin/grunt watch",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  },
  "jest": {
    "automock": false,
    "verbose": true,
    "bail": true,
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/mock/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg)$": "<rootDir>/mock/fileMock.js"
    }
  }
}
